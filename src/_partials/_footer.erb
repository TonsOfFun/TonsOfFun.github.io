<footer class="my-4">
  <div class="flex justify-center space-x-6">
    <% site.data.links.social.each do |social| %>
      <a class="text-teal-500 dark:text-teal-400 font-semibold" href="<%= social.url %>" class_names="text-zinc-700 dark:text-zinc-300" target="_blank" rel="noopener noreferrer">
        <span class="sr-only">social.title</span>
        <%= svg "/images/#{social.title}.svg", class: "w-5 h-5", "aria-hidden": true, width: 20, height: 20 %>
      </a>
    <% end %>
  </div>
</footer>
