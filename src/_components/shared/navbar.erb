<nav class="fixed top-0 z-20 w-full bg-zinc-100/90 dark:bg-zinc-800/90 drop-shadow" x-data="{
      open: false,
      get isOpen() { return this.open },
      toggle() { this.open = ! this.open },
    }">
  <div class="px-4 mx-auto max-w-7xl sm:px-6 lg:px-8">
    <div class="flex justify-between h-16">
      <div class="flex">
        <div class="flex items-center mr-2 -ml-2 md:hidden">
          <button @click="toggle" type="button" class="inline-flex items-center justify-center p-2 rounded-md text-zinc-700 dark:text-zinc-300 hover:bg-zinc-300 hover:text-zinc-800 dark:hover:bg-zinc-700 dark:hover:text-zinc-300 focus:outline-none focus:ring-2 focus:ring-inset focus:ring-zinc-50 dark:focus:ring-zinc-900" aria-controls="mobile-menu" aria-expanded="false">
            <span class="sr-only">Open main menu</span>
            <svg xmlns="http://www.w3.org/2000/svg" class="w-5 h-5" fill="none" viewBox="0 0 24 24" stroke="currentColor" aria-hidden="true" classes="block w-6 h-6" x-show="!isOpen">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 6h16M4 12h16M4 18h16"></path>
            </svg>
            <svg xmlns="http://www.w3.org/2000/svg" class="w-5 h-5" fill="none" viewBox="0 0 24 24" stroke="currentColor" aria-hidden="true" classes="hidden w-6 h-6" x-show="isOpen" style="display: none;">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M6 18L18 6M6 6l12 12"></path>
            </svg>
          </button>
        </div>
        <div class="hidden md:ml-6 md:flex md:items-center md:space-x-4">
          <!-- Current: "bg-zinc-900 text-white", Default: "text-zinc-300 hover:bg-zinc-700 hover:text-white" -->

          <% @data.links.navbar.each do |link| %>         
            <a class="hover:bg-zinc-300 hover:text-zinc-800 dark:hover:bg-zinc-700 dark:hover:text-zinc-300 font-semibold px-3 py-2 text-sm lg:text-base font-medium text-zinc-700 dark:text-zinc-300 rounded-md" href="<%= link.url %>" key="<%= link.title %>"><%= link.title %></a>
          <% end %>
        </div>
      </div>
      <div class="flex items-center space-x-4">
        <div class="flex-shrink-0">
          <a class="hover:bg-zinc-300 hover:text-zinc-800 dark:hover:bg-zinc-700 dark:hover:text-zinc-300 font-semibold" href="https://twitter.com/andrewmcodes/" class_names="inline-flex items-center justify-center p-2 rounded-md" target="_blank" rel="noopener noreferrer">
  <span class="sr-only">@andrewmcodes</span>
                <svg class="hover:text-[#1d9cf0] text-[#1DA1F2] w-5 h-5" fill="currentColor" viewBox="0 0 24 24" aria-hidden="true" width="20" height="20">
  <path fill-rule="evenodd" clip-rule="evenodd" d="M24 4.6c-.9.4-1.8.7-2.8.8 1-.6 1.8-1.6 2.2-2.7-1 .6-2 1-3.1 1.2-.9-1-2.2-1.6-3.6-1.6-2.7 0-4.9 2.2-4.9 4.9 0 .4 0 .8.1 1.1-4.2-.2-7.8-2.2-10.2-5.2-.5.8-.7 1.6-.7 2.5 0 1.7.9 3.2 2.2 4.1-.8 0-1.6-.2-2.2-.6v.1c0 2.4 1.7 4.4 3.9 4.8-.4.1-.8.2-1.3.2-.3 0-.6 0-.9-.1.6 2 2.4 3.4 4.6 3.4-1.7 1.3-3.8 2.1-6.1 2.1-.4 0-.8 0-1.2-.1 2.2 1.4 4.8 2.2 7.5 2.2 9.1 0 14-7.5 14-14v-.6c1-.7 1.8-1.6 2.5-2.5z"></path>
</svg>

</a>        </div>
        <div class="flex-shrink-0">
          <button x-data="{
            toggleDarkMode: () => {
              if (localStorage.theme === 'dark') {
                  localStorage.theme = 'light';
                  document.documentElement.classList.remove('dark');
              } else {
                  localStorage.theme = 'dark';
                  document.documentElement.classList.add('dark');
              }
            },
          }" @click="toggleDarkMode" aria-label="Toggle Darkmode" type="button" class="inline-flex items-center justify-center p-2 rounded-md text-zinc-700 dark:text-zinc-300 hover:bg-zinc-300 hover:text-zinc-800 dark:hover:bg-zinc-700 dark:hover:text-zinc-300 focus:outline-none focus:ring-2 focus:ring-inset focus:ring-zinc-50 dark:focus:ring-zinc-900">
  <span class="sr-only">Toggle theme</span>
  <svg xmlns="http://www.w3.org/2000/svg" class="w-5 h-5" width="20" height="20" viewBox="0 0 48 48" fill="currentColor">
  <path d="M22 41c10.493 0 19-8.507 19-19S32.493 3 22 3 3 11.507 3 22s8.507 19 19 19ZM7 22c0-8.284 6.716-15 15-15v30c-8.284 0-15-6.716-15-15Z"></path>
</svg>
</button>

        </div>
      </div>
    </div>
  </div>
  <div class="relative shadow-sm md:hidden" id="mobile-menu" x-show="isOpen" style="display: none;">
    <div class="absolute w-full px-2 pt-2 pb-3 space-y-1 sm:px-3 bg-zinc-100 dark:bg-zinc-800">
      <!-- Current: "bg-zinc-900 text-white", Default: "text-zinc-300 hover:bg-zinc-700 hover:text-white" -->
      <% @data.links.navbar.each do |link| %>         
          <a class="hover:bg-zinc-300 hover:text-zinc-800 dark:hover:bg-zinc-700 dark:hover:text-zinc-300 font-semibold block px-3 py-2 text-base font-medium rounded-md text-zinc-700 dark:text-zinc-300" href="<%= link.url %>" key="<%= link.title %>"><%= link.title %></a>
      <% end %> 
    </div>
  </div>
</nav>